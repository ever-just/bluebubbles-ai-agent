# Node Environment
NODE_ENV=development
PORT=3000

# Claude API
ANTHROPIC_API_KEY=sk-ant-api03-...

# BlueBubbles Connection
BLUEBUBBLES_URL=http://localhost:1234
BLUEBUBBLES_PASSWORD=your-password-here

# Google OAuth
GOOGLE_CLIENT_ID=your-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-client-secret
REDIRECT_URI=http://localhost:3000/auth/google/callback

# Database
DATABASE_URL=postgresql://postgres:password@localhost:5432/bluebubbles_agent
REDIS_URL=redis://localhost:6379

# Security
ENCRYPTION_KEY=generate-64-character-hex-string-here
SESSION_SECRET=generate-strong-random-string-here

# Logging
LOG_LEVEL=info
