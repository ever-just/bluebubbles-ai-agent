# Anthropic API
ANTHROPIC_API_KEY=your_api_key_here
ANTHROPIC_MODEL=claude-3-5-haiku-latest
ANTHROPIC_MAX_CONCURRENT_REQUESTS=2
ANTHROPIC_SUMMARY_TRIGGER_TOKENS=4000
ANTHROPIC_CONTEXT_WINDOW_TOKENS=6000
ANTHROPIC_RESPONSE_MAX_TOKENS=350
ANTHROPIC_ENABLE_WEB_SEARCH=true
ANTHROPIC_ENABLE_WEB_FETCH=false
ANTHROPIC_WEB_SEARCH_MAX_USES=5
ANTHROPIC_WEB_FETCH_MAX_USES=3
ANTHROPIC_WEB_FETCH_BETA_HEADER=web-fetch-2025-09-10

# BlueBubbles
BLUEBUBBLES_URL=http://localhost:1234
BLUEBUBBLES_PASSWORD=your_bluebubbles_password

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/bluebubbles_agent

# Redis
REDIS_URL=redis://localhost:6379

# Google OAuth (Gmail & Calendar)
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_REDIRECT_URI=http://localhost:3000/auth/google/callback
# Production redirect URI (update in .env when deployed):
# GOOGLE_REDIRECT_URI=https://agent.everjust.co/auth/google/callback

# OpenAI (for Whisper audio transcription - optional)
OPENAI_API_KEY=your_openai_key_here

# GoDaddy API (DNS Management)
GODADDY_API_KEY=your_godaddy_api_key
GODADDY_API_SECRET=your_godaddy_api_secret
